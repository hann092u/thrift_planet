<div class="grid_1">
  <div class="baggrund"><slot /></div>
  <div class="overskrift"><h2>TIDLINJE FOR BUTIKKEN</h2></div>
  <div class="tidslinje"><img src="/omos/tidslinje2.svg" alt="tidslinje" /></div>
</div>

<style>
  .grid_1 {
    display: grid;
    grid-template-columns: 1fr;
    grid-row: 1fr 4fr;
  }

  .baggrund {
    width: 200%;
    grid-column: 1;
    grid-row: 1/3;
    z-index: -1;
    overflow: hidden;
  }

  .overskrift {
    grid-column: 1;
    grid-row: 1;
    margin-top: 20px;
  }

  .tidslinje {
    grid-column: 1;
    grid-row: 2;
    margin-left: 20px;
  }

  h2 {
    color: white;
    margin: 0 5px 0 5px;
  }
</style>
